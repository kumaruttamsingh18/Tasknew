<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uttam.task">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyAppMaster"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.PlayVideoActivity"></activity>
        <activity android:name=".activity.CourseDetailsActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.VideoListActivity" />
        <activity android:name=".activity.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>